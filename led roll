/*
Adesh Daga
led roll
*/

#include<xc.h>

void main()
{
    int i;
    TRISB=0;
    TRISC=0;
    while(1)
    {
        PORTB=0x4;
        for(i=30000;i>0;i++);
        PORTB=0x8;
        for(i=30000;i>0;i++);
       
       PORTC=0x1C;
       for(i=30000;i>0;i++);
       PORTC=0x38;
       for(i=30000;i>0;i++);
       PORTC=0x70;
       for(i=30000;i>0;i++);
       PORTC=0xE0;
       for(i=30000;i>0;i++);
       PORTC=0xC1;
       for(i=30000;i>0;i++);
       PORTC=0x83;
       for(i=30000;i>0;i++);

    }
}
